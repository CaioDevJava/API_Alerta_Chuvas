package api.inmet.newton.repository;

import java.util.Date;
import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;

import api.inmet.newton.mapper.AutomaticStationsMapper;

@Repository
public interface AutomaticStationRepository extends JpaRepository<AutomaticStationsMapper, String> {

	List<AutomaticStationsMapper> queryByDate (Date data);
}
