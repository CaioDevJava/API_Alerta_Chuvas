package api.inmet.newton.models;

import java.util.Date;

import javax.persistence.Entity;
import javax.persistence.Table;

import lombok.AllArgsConstructor;
import lombok.Getter;
import lombok.Setter;

@Getter
@Setter
@AllArgsConstructor
@Entity
@Table
public class AutomaticStationsFullEntity {

	private String CodEstacao;
	private Date data;
	private String horaUtc;
	private float precipTotalHora;
	private float pressaoAtmosfericaNvlEstacao;
	private float pressaoAtmosfericaMaxHora;
	private float pressaoAtmosfericaMinHora;
	private float radiacaoGlobal;
	private float tempDoAr;
	private float tempPontoOrvalho;
	private float tempMaxHora;
	private float tempMinHora;
	private float tempOrvalhoMaxHora;
	private float tempOrvalhoMinHora;
	private float umidRelMaxHora;
	private float umidRelMinHora;
	private float umidRelArHora;
	private float ventoDirecaoHora;
	private float ventoRajadaMax;
	private float ventoVelocHora;
}
